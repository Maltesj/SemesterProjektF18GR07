            <AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #e4f0d4;">
               <children>
                  <ImageView fitHeight="757.0" fitWidth="1015.0" layoutX="10.0" layoutY="6.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@sensum.png" />
                     </image>
                  </ImageView>
                  <ComboBox fx:id="chooseCase" layoutX="446.0" layoutY="40.0" onAction="#onaction" prefWidth="150.0" promptText="Case IDs" style="-fx-border-color: black; -fx-background-color: 77c6e5;" />
                  <Button layoutX="44.0" layoutY="94.0" mnemonicParsing="false" onAction="#startAssesment" prefHeight="42.0" prefWidth="170.0" style="-fx-background-color: #a0d3e5; -fx-border-color: #ff8f33; -fx-background-radius: 7; -fx-border-radius: 5; -fx-border-width: 3;" text="Start sagsvurdering">
                     <font>
                        <Font name="Calibri" size="18.0" />
                     </font></Button>
                  <Button layoutX="44.0" layoutY="153.0" mnemonicParsing="false" onAction="#showCaseInformationtEventHandler" prefHeight="42.0" prefWidth="170.0" style="-fx-background-color: #a0d3e5; -fx-border-color: white; -fx-background-radius: 5; -fx-border-radius: 5;" text="Vis sagsoplysning">
                     <font>
                        <Font name="Calibri" size="18.0" />
                     </font></Button>
                  <ListView layoutX="22.0" layoutY="293.0" prefHeight="187.0" prefWidth="110.0" style="-fx-background-color: 77c6e5;" />
